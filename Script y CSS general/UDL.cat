<script type="text/javascript">
$(document).ready(function() {
    $('[data-ride="carousel"]').swiperight(function() {
        $(this).carousel('prev');
    });
    $('[data-ride="carousel"]').swipeleft(function() {
        $(this).carousel('next');
    });

    $('[data-ride="carousel"] .carousel-control-acc.carousel-pause').on('focus', function() {
        $(this).parents('.carousel').carousel('pause');
    });
    $('[data-ride="carousel"] .carousel-control-acc.carousel-pause').on('click', function() {
        $(this).parents('.carousel').carousel('pause');
    });
    $('[data-ride="carousel"] .carousel-control-acc.carousel-play').on('click', function() {
        $(this).parents('.carousel').carousel('cycle');
    });
    $('[data-ride="carousel"] .carousel-indicators button').on('focus', function() {
        $(this).parents('.carousel').carousel('pause');
    });
    $('[data-ride="carousel"] .carousel-indicators button').on('blur', function() {
        $(this).parents('.carousel').carousel('cycle');
    });
    $('[data-ride="carousel"]').on('slide.bs.carousel', function (e) {
        var itemactive = e.relatedTarget;
        var parentitemactiveid = $(e.currentTarget).attr('id');
        $(parentitemactiveid + ' .item video').trigger('pause');
        $(itemactive).find('video').trigger('play');
    });
    // ajustar vídeos embebidos al alto y ancho del carousel

    if($('[data-ride="carousel"] .item .responsive-video').length) {
        $('[data-ride="carousel"] .item .responsive-video').each(function () {
            var carouselparentid = '#'+$(this).parents('[data-ride="carousel"]').attr('id');
            //console.log('carousel padre: '+carouselparentid);
            var carouwidthwindowforvideo = $(carouselparentid + ' .carousel-inner .item').width();
            //console.log('carousel ancho: ' + carouwidthwindowforvideo);
            var carouheightwindowforvideo = $(carouselparentid + ' .carousel-inner .item').height();
            //console.log('carousel alto: ' + carouheightwindowforvideo);
            var ratio = carouwidthwindowforvideo / carouheightwindowforvideo;
            if(ratio > 1.77){
                var carourationec = 1.77;//ratio de formato 16:9
                var carounecheight = carouwidthwindowforvideo / carourationec;
                var caroudifheight = carouheightwindowforvideo - carounecheight;
                var caroumodiftop = caroudifheight / 2;
                //if($(carouselparentid + ' .item .responsive-video .embed-responsive iframe').length) {
                $(carouselparentid + ' .item .responsive-video .embed-responsive').css({
                    'top' : '0px',
                    'height': '100%'
                });
                //}
                //if($(carouselparentid + ' .item .responsive-video video').length) {
                $(carouselparentid + ' .item .responsive-video video').css({
                    'top' : '0px'
                });
                //}
            }else {
                var carourationec = 1.77;//ratio de formato 16:9
                var carounecwidth = carouheightwindowforvideo * carourationec;
                var caroudifwidth = carouwidthwindowforvideo - carounecwidth;
                var caroumodifleft = caroudifwidth / 2;
                $(carouselparentid + ' .item .responsive-video .embed-responsive').css({
                    'left' : '0px',
                    'width': 'auto'
                });
                if($(carouselparentid + ' .item .responsive-video video').length) {
                    $(carouselparentid + ' .item .responsive-video video').css({
                        'left' : '0px',
                        'width': 'auto'
                    });
                }
            }
        });
    }

    $(document).on('click', '.carousel-control', function () {
        var CarouselDestino = $(this).attr('data-slider');

        switch ($(this).attr('data-slide')) {
            case "prev":
                $(CarouselDestino).carousel('prev');
                break;
            case "next":
                $(CarouselDestino).carousel('next');
                break;
        }

    });
});




